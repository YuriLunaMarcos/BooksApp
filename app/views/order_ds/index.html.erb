<h1>Listing order_ds</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Product</th>
      <th>Acuerdo</th>
      <th>Estado</th>
      <th>Cantidad</th>
      <th>Desceuento</th>
      <th>Preciounitario</th>
      <th>Importetotal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_ds.each do |order_d| %>
      <tr>
        <td><%= order_d.order_id %></td>
        <td><%= order_d.product_id %></td>
        <td><%= order_d.acuerdo %></td>
        <td><%= order_d.estado %></td>
        <td><%= order_d.cantidad %></td>
        <td><%= order_d.desceuento %></td>
        <td><%= order_d.precioUnitario %></td>
        <td><%= order_d.importeTotal %></td>
        <td><%= link_to 'Show', order_d %></td>
        <td><%= link_to 'Edit', edit_order_d_path(order_d) %></td>
        <td><%= link_to 'Destroy', order_d, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

